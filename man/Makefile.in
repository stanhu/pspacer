MANDIR		:= $(DESTDIR)/usr/share/man

INSTALL		:= @INSTALL@
INSTALL_PROGRAM	:= @INSTALL_PROGRAM@
INSTALL_SCRIPT	:= @INSTALL_SCRIPT@
INSTALL_DATA	:= @INSTALL_DATA@

all:

install:
	test -d $(MANDIR)/man8 || mkdir -p $(MANDIR)/man8
	$(INSTALL_DATA) man8/tc-psp.8 $(MANDIR)/man8
